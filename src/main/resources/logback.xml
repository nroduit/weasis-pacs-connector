<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{ISO8601} [%15.15t] %-5level %-25.25logger{1} %msg%n%ex{5}</pattern>
        </encoder>
    </appender>

    <logger name="org.apache.catalina.access" level="WARN"/>
    <logger name="org.dcm4che3.net" level="WARN"/>
    <logger name="org.weasis" level="INFO"/>

    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>
